{
  "expo": {
    "name": "hymnos",
    "slug": "hymnos",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": "true",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bishoyroufael.hymnos"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.bishoyroufael.hymnos"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bunder": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router"
    ]
  }
}
